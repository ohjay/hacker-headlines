{
  "manifest_version": 2,

  "name": "Hacker Headlines",
  "description": "This extension displays the highest-rated hacker news",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/hh_icon.png"
  },
  "permissions": [
    "notifications",
    "alarms"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}